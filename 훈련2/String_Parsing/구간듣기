import java.util.*;
public class 구간듣기 {

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        String song = sc.next();
        int a, b, c;
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();

        String fav = song.substring(a, b+1);
        for(int i = 0; i < c; i++){
            System.out.print(fav);
        }

        sc.close();
    }//main./
}